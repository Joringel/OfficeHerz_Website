{{#content "stage"}}
	{{#embed "patterns.organism.stage.stage-pattern" stage-class="" stage-image-path=(data 'images.stage-1')}}
		{{#content "stage-image"}}
			<video autoplay loop muted class="stage__video running" onplay="this.classList.add('running')">
				<source src="/images/_stage/02_MotionControl.mp4">
			</video>
		{{/content}}
		{{#content "stage-header"}}
			{{#embed "patterns.atom.headline.headline-pattern" headline-size="1"}}
				{{#content "headline"}}
					Trinamic’s advanced microsystems transform digital information into physical motion.
				{{/content}}
			{{/embed}}
		{{/content}}
		{{#content "stage-footer"}}
			{{#embed "patterns.atom.button.button-pattern" button-class="btn-outline-white btn--icon-right" button-icon="triangle" button-label="What is Motion Control?" button-link="#" button-start-tag="a"}}
			{{/embed}}
		{{/content}}
	{{/embed}}
{{/content}}
{{#embed "patterns.atom.section.section-pattern" section-class=" "}}
	{{#content "section-body"}}
		<div class="container">
			<div class="row">
				<div class="col-md-12 col-lg-8 offset-lg-2">
					{{#embed "patterns.atom.headline.headline-pattern" headline-size="2" headline-class="mb-1 mb-md-2"}}
						{{#content "headline"}}
							Trinamic Motion Control
						{{/content}}
					{{/embed}}
					{{#embed "patterns.atom.paragraph.paragraph-pattern" paragraph-class="mb-0"}}
						{{#content "paragraph"}}
							Trinamic develops the world's most sophisticated technology for motion and motor control applications. Our state-of-the-art ICs, modules, and mechatronic systems enable today's software engineers to quickly and reliably develop highly precise drives that work efficiently, smoothly, and quietly.
						{{/content}}
					{{/embed}}
				</div>
			</div>
		</div>
	{{/content}}
{{/embed}}
