								{{/content}}
							{{/embed}}
						{{/content}}
					{{/embed}}
				</div>
			</div>
			<div class="row">
				<div class="col-12 col-lg-10 offset-lg-1">
					{{#embed "patterns.molecule.table.table-pattern"}}
