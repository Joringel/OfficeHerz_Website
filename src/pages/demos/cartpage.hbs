							<tr>
								{{#embed "patterns.molecule.table._table-item-header" table-item-header-label="Productname"}}
								{{/embed}}
								{{#embed "patterns.molecule.table._table-item-header" table-item-header-label="Quantity" }}
								{{/embed}}
								{{#embed "patterns.molecule.table._table-item-header" table-item-header-label="Delete" }}
								{{/embed}}
							</tr>
						{{/content}}
						{{#content "table-body"}}
							{{#embed "patterns.molecule.table._table-item-row"}}
								{{#content "table-item-row-body"}}
									{{#embed "patterns.molecule.table._table-item-column" table-item-column-link="#" table-item-column-label="TMC2041"}}
									{{/embed}}
									{{#embed "patterns.molecule.table._table-item-column"}}
										{{#content table-item-column}}
											{{#embed "patterns.atom.form-text.form-text-pattern" form-text-value="2"}}
											{{/embed}}
										{{/content}}
									{{/embed}}
									{{#embed "patterns.molecule.table._table-item-column"}}
										{{#content table-item-column}}
											{{#embed "patterns.atom.button.button-pattern" button-class="btn-link btn--icon" button-icon="trash" button-label="" button-link="#" button-start-tag="a"}}
											{{/embed}}
										{{/content}}
									{{/embed}}
								{{/content}}
							{{/embed}}
							{{#embed "patterns.molecule.table._table-item-row"}}
							{{#content "table-item-row-body"}}
								{{#embed "patterns.molecule.table._table-item-column" table-item-column-link="#" table-item-column-label="TMC2041"}}
								{{/embed}}
								{{#embed "patterns.molecule.table._table-item-column"}}
									{{#content table-item-column}}
										{{#embed "patterns.atom.form-text.form-text-pattern" form-text-value="2"}}
										{{/embed}}
									{{/content}}
								{{/embed}}
								{{#embed "patterns.molecule.table._table-item-column"}}
									{{#content table-item-column}}
										{{#embed "patterns.atom.button.button-pattern" button-class="btn-link btn--icon" button-icon="trash" button-label="" button-link="#" button-start-tag="a"}}
										{{/embed}}
									{{/content}}
								{{/embed}}
							{{/content}}
							{{/embed}}
							{{#embed "patterns.molecule.table._table-item-row"}}
								{{#content "table-item-row-body"}}
									{{#embed "patterns.molecule.table._table-item-column" table-item-column-link="#" table-item-column-label="TMC2041"}}
									{{/embed}}
									{{#embed "patterns.molecule.table._table-item-column"}}
										{{#content table-item-column}}
											{{#embed "patterns.atom.form-text.form-text-pattern" form-text-value="2"}}
											{{/embed}}
										{{/content}}
									{{/embed}}
									{{#embed "patterns.molecule.table._table-item-column"}}
										{{#content table-item-column}}
											{{#embed "patterns.atom.button.button-pattern" button-class="btn-link btn--icon" button-icon="trash" button-label="" button-link="#" button-start-tag="a"}}
											{{/embed}}
										{{/content}}
									{{/embed}}
								{{/content}}
							{{/embed}}
						{{/content}}
					{{/embed}}
				</div>
			</div>
		</div>
	{{/content}}
{{/embed}}

