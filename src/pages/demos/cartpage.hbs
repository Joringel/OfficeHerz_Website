{{#content "stage"}}
	{{#embed "patterns.organism.stage.stage-pattern" stage-class="stage--type-a" stage-image-path=(data 'images.stage-4')}}
		{{#content "stage-breadcrumb"}}
			{{#embed "patterns.atom.breadcrumb.breadcrumb-pattern"}}
			{{/embed}}
		{{/content}}
		{{#content "stage-header"}}
			{{#embed "patterns.atom.headline.headline-pattern" headline-size="1"}}
				{{#content "headline"}}
					Your Cart
				{{/content}}
			{{/embed}}
		{{/content}}
	{{/embed}}
{{/content}}

{{#embed "patterns.atom.section.section-pattern"}}
	{{#content "section-body"}}
		<div class="container">
			<div class="row">
				<div class="col-12 offset-lg-1 col-lg-10">
					{{#embed "patterns.atom.section-header.section-header-pattern" section-header-class="section-header--align-left"}}
						{{#content "section-header-body"}}
							{{#embed "patterns.atom.headline.headline-pattern" headline-size="2" headline-class="mb-2"}}
