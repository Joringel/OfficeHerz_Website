---
title: Icons
layout: colors
---

<div class="container-fluid">
   <div class="row">
    {{#each (data "icons")}}
        <div class="col-2">
            <div class="d-flex justify-content-center">
                {{#embed "patterns.atom.icon.icon-pattern" icon-class="" icon-name=icon-name}}{{/embed}}
            </div>
            <div class="detail detail-js mt-2 mb-4 text-center">
                {{icon-name}}
            </div>
        </div>
    {{/each}}
  </div>
</div>

