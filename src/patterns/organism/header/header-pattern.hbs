---
name: header
header-class: bg-faded
hidden: true
---

<header class="header {{header-class}}">
	<div class="header_header">
		{{#block "header-header"}}
			<div class="container">
				{{#embed "patterns.molecule.nav.nav-pattern" nav-class="justify-content-end"}}
				{{#content "nav-body"}}
					{{#embed "patterns.molecule.nav._nav-item-pattern" nav-item-class="d-flex nav-item-header--left" nav-item-label="SERVICEHOTLINE: 0 22 43 / 88 99 5" nav-icon-name="phone"}}{{/embed}}

					{{#embed "patterns.atom.dropdown.dropdown-pattern" dropdown-label="Privatkunde" dropdown-button-class="btn-secondary btn-secondary--header d-flex justify-content-center"}}
						{{#content "dropdown-menue"}}
						<a class="dropdown-item" href="#">Privatkunde</a>
						<a class="dropdown-item" href="#">Geschäftskunde</a>
						{{/content}}
					{{/embed}}

					{{#embed "patterns.molecule.nav._nav-item-pattern" nav-item-label="Anmelden" nav-icon-name="user"}}{{/embed}}
					{{#embed "patterns.molecule.nav._nav-item-pattern" nav-item-label="Sprache" nav-icon-name="flag"}}{{/embed}}
				{{/content}}
				{{/embed}}
			</div>
		{{/block}}
	</div>
	<div class="header_body">
		{{#block "header-body"}}

			{{#embed "patterns.organism.header._navbar" navbar-class="navbar-toggleable-md navbar-default d-flex flex-column"}}
				{{#content "navbar"}}
						{{#embed "patterns.organism.search.search-pattern" search-class="search--navbar"}}
							{{#content "search-header"}}
								{{#embed "patterns.molecule.filter.filter-pattern" filter-class="filter--navbar"}}
									{{#content "filter-body"}}
										<div class="filter-row">
											{{#embed "patterns.molecule.form-group.form-group-pattern" form-group-class="mb-0 d-inline-flex" form-text-type="text" form-text-placeholder="Suchen Sie nach Produktname, Artikelnummer, Kategorie"}}
											{{/embed}}
										</div>
									{{/content}}
								{{/embed}}
							{{/content}}
						{{/embed}}

						{{#embed "patterns.molecule.nav.nav-pattern" nav-class="justify-content-end"}}
							{{#content "nav-body"}}
								{{#embed "patterns.molecule.nav._nav-item-pattern" nav-item-class="" nav-item-label="Merkzettel" nav-icon-name="heart-o"}}{{/embed}}
								{{#embed "patterns.molecule.nav._nav-item-pattern" nav-item-class="" nav-item-label="Warenkorb" nav-icon-name="shopping-basket"}}{{/embed}}
							{{/content}}
						{{/embed}}

						<div class="navbar-dropdown-menue">
						{{#embed "patterns.molecule.nav.nav-pattern" nav-class="navbar-nav mx-lg-2 mx-xl-6"}}
						{{#content "nav-body"}}
						<li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Schreiben & Korrigieren</a>
