---
name: Image Pattern
image-path: /images/placeholder-ratio.jpg
previewClass: col-6
hidden: true
---
<div class="image {{image-class}}">
  <img src="{{image-path}}" alt="{{#if image-alt}}{{image-alt}}{{else}}img-alt{{/if}}" {{#if image-width}}width="{{image-width}}"{{/if}} {{#if image-height}}height="{{image-height}}"{{/if}} />
</div>
